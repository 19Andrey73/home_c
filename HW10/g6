#include <stdio.h>
#include <string.h>
#include <stdbool.h>

bool is_palindrom(char str[]) {
    int left = 0;
    int right = strlen(str) - 1;
    
    while (left < right) {
        if (str[left++] != str[right--]) {
            return false;
        }
    }
    return true;
}

int main() {
    FILE *input_file, *output_file;
    char str[1001];
    
    input_file = fopen("input.txt", "r");
    if (input_file == NULL) return 1;
    
    fscanf(input_file, "%s", str);
    fclose(input_file);
    
    output_file = fopen("output.txt", "w");
    if (output_file == NULL) return 1;
    
    fprintf(output_file, is_palindrom(str) ? "YES" : "NO");
    fclose(output_file);
    
    return 0;
}
