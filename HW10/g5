#include <stdio.h>
#include <string.h>

int main() {
    FILE *input_file = fopen("input.txt", "r");
    FILE *output_file = fopen("output.txt", "w");
    
    if (input_file == NULL || output_file == NULL) {
        printf("error!\n");
        return 1;
    }
    
    char str[1001];
    
    // Чтение строки из файла
    if (fgets(str, 1001, input_file) == NULL) {
        printf("error!\n");
        return 1;
    }
    
    // Удаление символа новой строки, если он есть
    int len = strlen(str);
    if (len > 0 && str[len-1] == '\n') {
        str[len-1] = '\0';
        len--;
    }
    
    // Замена символов
    for (int i = 0; i < len; i++) {
        switch (str[i]) {
            case 'a': str[i] = 'b'; break;
            case 'b': str[i] = 'a'; break;
            case 'A': str[i] = 'B'; break;
            case 'B': str[i] = 'A'; break;
        }
    }
    
    // Запись результата в файл
    fprintf(output_file, "%s", str);
    
    fclose(input_file);
    fclose(output_file);
    
    return 0;
}
